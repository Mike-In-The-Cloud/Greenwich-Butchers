@model int
@using GreButchersEFCore_V2.Utility
@*
    For more information on enabling MVC for empty projects, visit http://go.microsoft.com/fwlink/?LinkID=397860
*@
<td style="width:150px">
    <div class="btn-group" role="group">
        @if (User.IsInRole(StaticDetails.AdminUser) || User.IsInRole(StaticDetails.SuperAdminUser))
        {
            <a type="button" class="btn btn-primary" href="@Url.Content("BulkOrder/Edit/" + Model)">
                <i class="fas fa-edit"></i>
            </a>
        }   <a type="button" class="btn btn-success" href="@Url.Content("BulkOrder/Details/"+Model)">
            <i class="far fa-list-alt"></i>
        </a>
        <a type="button" class="btn btn-danger" href="@Url.Content("BulkOrder/Delete/"+Model)">
            <i class="fas fa-trash-alt"></i>
        </a>
    </div>
</td>
