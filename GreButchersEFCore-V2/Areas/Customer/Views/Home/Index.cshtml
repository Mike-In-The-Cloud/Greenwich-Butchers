@model List<GreButchersEFCore_V2.Models.Product>

@{
    ViewData["Title"] = "Greenwich Butchers";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<br /> <br />
<div class="border backgroundWhite">
